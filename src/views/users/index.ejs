<h2>ユーザー一覧</h2>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <img src="public/images/kirin.png" alt="1" width="100" height="100" />
    <% users.forEach(user => { %>
      <tr>
        <td>
          <%= user.name %>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>
